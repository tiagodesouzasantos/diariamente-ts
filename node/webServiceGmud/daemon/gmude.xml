<service>
	<id>gmude.exe</id>
	<name>Gmude</name>
	<description>Projeto destinado para atualização dos repositórios com projetos microled</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\rdp-tiago\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\node\projetos\webServiceGmud\gmud-server.js</argument>
	<argument>--log</argument>
